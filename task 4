#include <iostream>

int add(int a, int b){
return a + b;
}

double sub(double u, double k){
return u - k;
}

double mul(double u, double k){
return u * k;
}

int divide(int a, int b){
return a / b;
}

int main(){

	int a = 15;
	int b = 3;
	double u = 10.55;
	double k = 7.35;

std::cout << add(a, b) << " " << sub(u, k) << " " << mul(u, k) << " " << divide(a, b);

}
