#include <iostream>

int FindMin(int *arr, int size){
int min = arr[0];
    for(int i = 1; i < size; ++i){
        if(arr[i] < min){
        min = arr[i];
        }
    }
return min;
}

int FindMax(int *arr, int size){
int max = arr[0];
    for(int i = 1; i < size; ++i){
        if(arr[i] > max){
        max = arr[i];
        }
    }
return max;
}

int Average(int *arr, int size){
int sum = 0;
    for(int i = 0; i < size; ++i){
    sum += arr[i];
}
return sum/size;
}


int main() {
int size;
std::cin >> size;

int arr[size];

for(int i = 0; i < size; ++i){
arr[i] = i + 1;
}

std::cout << FindMin(arr, size) << std::endl;
std::cout << FindMax(arr, size) << std::endl;
std::cout << Average(arr, size) << std::endl;

}
